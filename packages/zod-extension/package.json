{
  "name": "@kui/zod-extension",
  "version": "0.0.1",
  "description": "Extensões Zod para KUI Framework - Schemas declarativos para geração automática de UI",
  "keywords": ["zod", "schema", "validation", "typescript", "react", "forms", "kui"],
  "homepage": "https://github.com/kennyjsa/kui#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kennyjsa/kui.git",
    "directory": "packages/zod-extension"
  },
  "bugs": {
    "url": "https://github.com/kennyjsa/kui/issues"
  },
  "license": "MIT",
  "author": "Kenny JSA",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "zod": "^3.22.4"
  }
}

