{
  "name": "@kui/core",
  "version": "0.0.1",
  "description": "Core KUI Framework - Providers, registry e integração com backends (REST/tRPC)",
  "keywords": ["kui", "core", "provider", "trpc", "rest", "data", "backend"],
  "homepage": "https://github.com/kennyjsa/kui#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kennyjsa/kui.git",
    "directory": "packages/core"
  },
  "bugs": {
    "url": "https://github.com/kennyjsa/kui/issues"
  },
  "license": "MIT",
  "author": "Kenny JSA",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@kui/zod-extension": "workspace:*"
  },
  "peerDependenciesMeta": {
    "@trpc/client": {
      "optional": true
    },
    "@tanstack/react-query": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "react": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  }
}

