{
  "name": "@kui/forms",
  "version": "0.0.1",
  "description": "Formulários KUI Framework - Geração automática de formulários a partir de schemas Zod",
  "keywords": ["kui", "forms", "react-hook-form", "zod", "validation", "form-builder", "auto-form"],
  "homepage": "https://github.com/kennyjsa/kui#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kennyjsa/kui.git",
    "directory": "packages/forms"
  },
  "bugs": {
    "url": "https://github.com/kennyjsa/kui/issues"
  },
  "license": "MIT",
  "author": "Kenny JSA",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react --external react-hook-form",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external react --external react-hook-form",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@kui/core": "workspace:*",
    "@kui/ui": "workspace:*",
    "@kui/zod-extension": "workspace:*",
    "@hookform/resolvers": "^3.3.4",
    "react-hook-form": "^7.49.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.0.0"
  }
}

