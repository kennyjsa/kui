{
  "name": "@kui-framework/ui",
  "version": "0.0.1",
  "description": "Componentes UI KUI Framework - Biblioteca React com Radix UI e Tailwind CSS",
  "keywords": [
    "kui",
    "ui",
    "components",
    "react",
    "radix-ui",
    "tailwind",
    "design-system"
  ],
  "homepage": "https://github.com/kennyjsa/kui#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/kennyjsa/kui.git",
    "directory": "packages/ui"
  },
  "bugs": {
    "url": "https://github.com/kennyjsa/kui/issues"
  },
  "license": "MIT",
  "author": "Kenny JSA",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --external react",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch --external react",
    "lint": "eslint src/",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@kui-framework/theme": "workspace:*",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toast": "^1.2.15",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "lucide-react": "^0.546.0",
    "react-dropzone": "^14.3.8",
    "react-input-mask": "^2.0.4",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/react-input-mask": "^3.0.6",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  }
}
